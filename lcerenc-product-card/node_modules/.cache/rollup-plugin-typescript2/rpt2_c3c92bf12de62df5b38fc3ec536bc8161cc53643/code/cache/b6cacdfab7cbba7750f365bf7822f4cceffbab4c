{"code":"import React, { useCallback, useContext } from \"react\";\r\nimport { ProductContext } from \"./ProductCard\";\r\nimport styles from \"../styles/styles.module.css\";\r\nconst ProductButtons = ({ className, style }) => {\r\n    const { counter, increaseBy, maxCount } = useContext(ProductContext);\r\n    const isMaxReached = useCallback(() => {\r\n        if (!maxCount)\r\n            return false;\r\n        return counter === maxCount;\r\n    }, [counter, maxCount]);\r\n    return (React.createElement(\"div\", { className: `${styles.buttonsContainer} ${className}`, style: style },\r\n        React.createElement(\"button\", { className: styles.buttonMinus, onClick: () => increaseBy(-1) }, \"-\"),\r\n        React.createElement(\"div\", { className: styles.countLabel }, counter),\r\n        React.createElement(\"button\", { className: `${styles.buttonAdd} ${isMaxReached() && 'disabled'}`, onClick: () => increaseBy(1) }, \"+\")));\r\n};\r\nexport default ProductButtons;\r\n//# sourceMappingURL=ProductButtons.js.map","references":["/home/projects/02-ReactPro/01-react-app/lcerenc-product-card/node_modules/@types/react/ts5.0/index.d.ts","/home/projects/02-ReactPro/01-react-app/lcerenc-product-card/src/components/ProductCard.tsx"],"map":"{\"version\":3,\"file\":\"ProductButtons.js\",\"sourceRoot\":\"\",\"sources\":[\"../../../../../src/components/ProductButtons.tsx\"],\"names\":[],\"mappings\":\"AAAA,OAAO,KAAK,EAAE,EAAE,WAAW,EAAE,UAAU,EAAE,MAAM,OAAO,CAAC;AACvD,OAAO,EAAE,cAAc,EAAE,MAAM,eAAe,CAAC;AAC/C,OAAO,MAAM,MAAM,6BAA6B,CAAC;AAOjD,MAAM,cAAc,GAAG,CAAC,EAAE,SAAS,EAAE,KAAK,EAAS,EAAE,EAAE;IACrD,MAAM,EAAE,OAAO,EAAE,UAAU,EAAE,QAAQ,EAAE,GAAG,UAAU,CAAC,cAAc,CAAC,CAAC;IAErE,MAAM,YAAY,GAAG,WAAW,CAAC,GAAE,EAAE;QACnC,IAAG,CAAC,QAAQ;YAAE,OAAO,KAAK,CAAC;QAC3B,OAAO,OAAO,KAAK,QAAQ,CAAA;IAC7B,CAAC,EAAE,CAAC,OAAO,EAAE,QAAQ,CAAC,CAAC,CAAC;IACxB,OAAO,CACL,6BAAK,SAAS,EAAE,GAAG,MAAM,CAAC,gBAAgB,IAAI,SAAS,EAAE,EAAE,KAAK,EAAE,KAAK;QACrE,gCAAQ,SAAS,EAAE,MAAM,CAAC,WAAW,EAAE,OAAO,EAAE,GAA